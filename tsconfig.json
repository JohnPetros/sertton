{
  "extends": "expo/tsconfig.base",
  "include": [
    "**/*",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "compilerOptions": {
    "strict": true,
    "module": "ES2020",
    "baseUrl": ".",
    "paths": {
      "@/@types/*": [
        "src/@types/*"
      ],
      "@/app/*": [
        "src/app/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/providers/*": [
        "src/providers/*"
      ],
      "@/stores/*": [
        "src/stores/*"
      ],
      "@/services/*": [
        "src/services/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ],
      "@/__tests__/*": [
        "src/_tests_/*"
      ]
    }
  }
}
